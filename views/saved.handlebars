<div class="jumbotron" style="background-image: url(http://www.digitalmeetsculture.net/wp-content/uploads/2015/04/article.jpg); background-size: 100%; height:400px;">
   <div class="container for-about">
   <h1 class="text-center" style="color:cornflowerblue">Saved Articles</h1>
   <h1 class="text-center"></h1>
   
   </div>
</div>


  {{#each articles}}
        <br><br><br>

    <div class="backcard">
      <div>
        <span>
          <p  class="headercard savedlist">Headline : {{this.headline}}</p>
          <button class="btn btn-primary float-right note" style="float:right;margin:5px;" data-id="{{this._id}}">ARTICLE NOTE</button>
    
          <button class="btn btn-primary float-right unsaved" style="float:right;margin:5px;" data-id="{{this._id}}">Remove From Saved</button>

        </span>
        <p class="detailstext">Summary : {{this.summury}}</p>
        <p class="detailstext"><a class="link" href="https://www.nytimes.com{{this.url}}" target="_blank" >Click Here To See More Details !!!</a></p>
      </div>
    
      </div>
    </div>
    
    {{/each}}
  <script src="./saved.js"></script>
